{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "open-street-cam": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/.htaccess",
              "src/android-chrome-192x192.png",
              "src/android-chrome-512x512.png",
              "src/apple-touch-icon.png",
              "src/browserconfig.xml",
              "src/favicon-16x16.png",
              "src/favicon-32x32.png",
              "src/manifest.json",
              "src/mstile-144x144.png",
              "src/mstile-150x150.png",
              "src/mstile-310x150.png",
              "src/mstile-310x310.png",
              "src/mstile-70x70.png",
              "src/safari-pinned-tab.svg"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/leaflet/dist/leaflet.css",
              "src/assets/css/leaflet-areaselect.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/tether/dist/js/tether.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/leaflet/dist/leaflet.js",
              "node_modules/leaflet.vectorgrid/dist/Leaflet.VectorGrid.bundled.js",
              "node_modules/leaflet-polylinedecorator/dist/leaflet.polylineDecorator.js",
              "node_modules/osm-auth/osmauth.js",
              "src/assets/js/GeoJSONGridLayer.js",
              "src/assets/js/leaflet-areaselect.js",
              "src/assets/js/leaflet-canvasoverlay.js",
              "src/assets/js/leaflet-canvaslayer.js",
              "src/assets/js/osc-leaflet.js"
            ]
          },
          "configurations": {
            "github": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.github.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "open-street-cam:build"
          },
          "configurations": {
            "github": {
              "browserTarget": "open-street-cam:build:github"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "open-street-cam:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/tether/dist/js/tether.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/leaflet/dist/leaflet.js",
              "node_modules/leaflet.vectorgrid/dist/Leaflet.VectorGrid.bundled.js",
              "node_modules/leaflet-polylinedecorator/dist/leaflet.polylineDecorator.js",
              "node_modules/osm-auth/osmauth.js",
              "src/assets/js/GeoJSONGridLayer.js",
              "src/assets/js/leaflet-areaselect.js",
              "src/assets/js/leaflet-canvasoverlay.js",
              "src/assets/js/leaflet-canvaslayer.js",
              "src/assets/js/osc-leaflet.js"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/leaflet/dist/leaflet.css",
              "src/assets/css/leaflet-areaselect.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "src/styles.css"
            ],
            "assets": [
              "src/assets",
              "src/.htaccess",
              "src/android-chrome-192x192.png",
              "src/android-chrome-512x512.png",
              "src/apple-touch-icon.png",
              "src/browserconfig.xml",
              "src/favicon-16x16.png",
              "src/favicon-32x32.png",
              "src/manifest.json",
              "src/mstile-144x144.png",
              "src/mstile-150x150.png",
              "src/mstile-310x150.png",
              "src/mstile-310x310.png",
              "src/mstile-70x70.png",
              "src/safari-pinned-tab.svg"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "open-street-cam-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "open-street-cam:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "open-street-cam",
  "cli": {
    "warnings": {
      "typescriptMismatch": false
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "osc",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "osc"
    }
  }
}