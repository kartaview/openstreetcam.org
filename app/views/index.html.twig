    <script type="text/html" id="tpl_dnearSequencesetails">
        <div class="container-fluid">
            <div class="row">
                
                <div class="col-lg-9 image-container">
                    
                    <h2>Track uploader: <span><%= user %></span></h2>
                    <div class="image checkersBg" style="height: 80%;">
                        <div class="actions">
                            <ul>
                                <li data-toggle="tooltip" data-placement="right" title="Adjust blurring">
                                    <a href="javascript:void(0);" onclick="$('#UNAVAILABLE_MODAL').modal('show');"><i class="fa fa-magic" aria-hidden="true"></i></a>
                                </li>
                                <li data-toggle="tooltip" data-placement="right" title="Edit road signs">
                                    <a href="javascript:void(0);" onclick="$('#UNAVAILABLE_MODAL').modal('show');"><i class="fa fa-map-signs" aria-hidden="true"></i></a>
                                </li>
                                <li data-toggle="tooltip" data-placement="right" title="Edit in JOSM">
                                    <a href="javascript:void(0);" class="josm"><i class="fa fa-pencil" aria-hidden="true"></i></a>
                                </li>
			   <% if(owner || isSuperUser) { %>
                                <li data-toggle="tooltip" data-placement="right" title="Rotate">
                                    <a href="javascript:void(0);" id="rotate-forward"><i class="fa fa-repeat" aria-hidden="true"></i></a>
                                </li>
                                <li data-toggle="tooltip" data-placement="right" title="Rotate">
                                    <a href="javascript:void(0);" id="rotate-backward"><i class="fa fa-undo" aria-hidden="true"></i></a>
                                </li>
                                <li style="display:none" id="save-button-set" data-toggle="tooltip" data-placement="right" title="Save changes">
                                    <a href="javascript:void(0);" data-toggle="modal" data-target="#save-rotation-dialog"><i class="fa fa-check" aria-hidden="true"></i></a>
                                </li>
                            </ul>
                            <ul>
                                <li data-toggle="tooltip" data-placement="right" title="Delete photo">
                                    <a href="javascript:void(0);" id="remove"  data-toggle="modal" data-target="#delete-photo-dialog"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
                                </li>
                            </ul>
			<% } %>
                        </div>
                        <canvas id="streetGallery" class="streetGallery"></canvas>
                        <div class="photo-count"><i class="fa fa-camera" aria-hidden="true"></i> <span class="marker-index"><%=markerIndex %></span> <span class="grey-color">/</span> <span class="grey-color"><%= totalPhotos %></span></div>
                        <div class="move">
                            <div class="row">
                                <div class="col-md-2">
                                    <a href="javascript:void(0);"><i class="fa fa-step-backward" aria-hidden="true"></i></a>
                                    <a href="javascript:void(0);"><i class="fa fa-step-forward" aria-hidden="true"></i></a>
                                </div>
                                <div class="col-md-10">
                                    <div class="progress float-right">
                                      <div class="progress-edge left"></div>
                                      <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">
                                      </div>
                                      <span class="progress-pointer"></span>
                                      <div class="progress-edge right"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                </div>
                <div class="col-lg-3">
                    <h2>Location: <span class="locationLat"><%=photos[markerIndex].lat%></span> | <span class="locationLng"><%= photos[markerIndex].lng %></span></h2>
                    <div id="mini-map"></div>
                    <div class="info">
                        <h4>Info</h4>
                        <div class="col-md-6">Track uploader:</div><div class="col-md-6"><%= user %></div>
                        <div class="col-md-6">Date of recording:</div><div class="col-md-6"><%= date %></div>
                        <div class="col-md-6">Track no.:</div><div class="col-md-6"><%= routeId %></div>
                        <div class="col-md-6">Platform:</div><div class="col-md-6"><%= platform || 'Unknown' %></div>
                        <div class="col-md-6">Total no. of images:</div><div class="col-md-6"><%= totalPhotos %></div>
                    </div>
                    
                </div>
                
            </div>
        </div>
    </script>
    
