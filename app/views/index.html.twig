    <script type="text/html" id="tpl_nearsequences">
        <div class='nearsequences_header container-fluid'>
            <div class='close-btn'>
                <i class="fa fa-times" aria-hidden="true"></i>
            </div>
            Tracks
        </div>
        <div class='nearsequences container-fluid'>
            <div class='previous'><i class="fa fa-angle-left" aria-hidden="true"></i></div>
            <% _.each(sequences, function(sequence){ %>
            <div class="photo" data-sequence-id="<%= _.escape(sequence.sequence_id) %>"
                 data-sequence-index="<%= _.escape(sequence.sequence_index) %>">
                 <div class='infoPlaceholder'>
                    <p><span class='highlight address'><%= _.escape(sequence.address) %></span></p>
                    <p>
                        <span class='highlight'><i class="fa fa-camera" aria-hidden="true"></i><%= _.escape(sequence.photo_no) %></span><span>IMG</span>
                        <span class='highlight'><i class="fa fa-inr" aria-hidden="true"></i><%= _.escape(sequence.distance) %></span><span>KM</span>
                        <span class='highlight'><%= _.escape(sequence.date) %></span><span> | <%= _.escape(sequence.hour) %></span>
                    </p>
                 </div>
                 <div class='photoPlaceholder checkersBg'>
                    <span class='helper'></span><img src="<%= _.escape(sequence.photo) %>" />
                 </div>
            </div>
            <% }); %>
            <div class='next'><i class="fa fa-angle-right" aria-hidden="true"></i></div>
        </div>
    </script>
