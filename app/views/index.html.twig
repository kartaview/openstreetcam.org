        <% _.each(trackList, function(sequence, j){ var date = new Date(sequence.date_added) %>
            <% if (j == 0) { %>
                <div class="row">
            <% } %>
            <% if (j != 0 && j%6 == 0) { %>
                </div><div class="row">
